include $(CLEAR_VARS)
LOCAL_MODULE := android-check
include $(BUILD_SHARED_LIBRARY)
$(info ANDROID> TOOLCHAIN_PREFIX = $(TOOLCHAIN_PREFIX))
$(info ANDROID> TOOLCHAIN_PREBUILT_ROOT = $(TOOLCHAIN_PREBUILT_ROOT))
$(info ANDROID> NDK_TOOLCHAIN_VERSION = $(NDK_TOOLCHAIN_VERSION))
$(info ANDROID> AR = $(TARGET_AR))
$(info ANDROID> RANLIB = $(TARGET_RANLIB))
$(info ANDROID> LD = $(TARGET_LD))
$(info ANDROID> CC = $(TARGET_CC))
$(info ANDROID> CXX = $(TARGET_CXX))
$(info ANDROID> CFLAGS = $(TARGET_CFLAGS) $(TARGET_NO_EXECUTE_CFLAGS))
$(info ANDROID> CXXFLAGS = $(TARGET_CXXFLAGS) $(TARGET_NO_EXECUTE_CFLAGS))
$(info ANDROID> LDFLAGS = $(TARGET_LDFLAGS) $(TARGET_NO_EXECUTE_LDFLAGS) $(TARGET_NO_UNDEFINED_LDFLAGS))
$(info ANDROID> INCLUDES = $(TARGET_C_INCLUDES) $(__ndk_modules.$(APP_STL).EXPORT_C_INCLUDES))
$(info ANDROID> LIBS = $(TARGET_LDLIBS) $(TARGET_LIBGCC) -l$(NDK_STL.$(APP_STL).SHARED_LIBS))
$(info ANDROID> debug_FLAGS = $(TARGET_$(TARGET_ARCH)_debug_CFLAGS))
$(info ANDROID> release_FLAGS = $(TARGET_$(TARGET_ARCH)_release_CFLAGS))
$(info ANDROID> SYSROOT = $(NDK_PLATFORM_$(NDK_APP_PLATFORM)_$(TARGET_ARCH)_SYSROOT))
$(info ANDROID> NDK_APP_PLATFORM = $(NDK_APP_PLATFORM))
$(info ANDROID> TARGET_ARCH = $(TARGET_ARCH))
